{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier:recommended", "plugin:pretter/recommended"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error"
  },
  "env": {
    "browser": true,
    "es2022": true,
    "jest": true,
    "node": true
  }
}